generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Users {
  id       Int     @id @default(autoincrement())
  username String  @unique
  password String
  files    Files[]
}

model Files {
  id        Int      @id @default(autoincrement())
  path      String
  createdAt DateTime @default(now())
  users     Users    @relation(fields: [userId], references: [id])
  userId    Int
}

model Session {
  id        String   @id
  sid       String   @unique
  data      String
  expiresAt DateTime
}
