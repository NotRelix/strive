<div class="file-card" onclick="openFileInfo()">
  <div class="file-name">
    <%- include("fileIcon", {file}) %>
    <span> <%= file.fileName %> </span>
  </div>
  <span class="file-size"><%= file.formatSize %></span>
  <span class="file-created-at"><%= file.formatCreatedAt %></span>
  <svg
    xmlns="http://www.w3.org/2000/svg"
    width="30"
    height="30"
    viewBox="0 0 24 24"
    fill="none"
    stroke="currentColor"
    stroke-width="2"
    stroke-linecap="round"
    stroke-linejoin="round"
    class="menu-btn"
  >
    <circle cx="12" cy="12" r="1" />
    <circle cx="12" cy="5" r="1" />
    <circle cx="12" cy="19" r="1" />
  </svg>
</div>
